{
  "name": "therapy-website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "kill-port 3000 && next dev --turbo -p 3000",
    "dev:standard": "kill-port 3000 && next dev -p 3000",
    "dev:3000": "kill-port 3000 && next dev --turbo -p 3000",
    "dev:3001": "kill-port 3001 && next dev --turbo -p 3001",
    "dev:free": "kill-port 3000 3001 3002 3003 3004 3005 3006 3007 3008 3009 3010",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test:e2e": "npm run dev & wait-on http://localhost:3000 && playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:update-snapshots": "playwright test --update-snapshots",
    "lhci": "lhci autorun",
    "links": "linkinator http://localhost:3000 --recurse --timeout 120000 --concurrency 2 --skip 'https?://' --skip '/_next/'",
    "lhci:collect": "lhci collect",
    "lhci:assert": "lhci assert",
    "mcp:content": "node ./mcp-servers/content-audit-server.js",
    "mcp:seo": "node ./mcp-servers/lighthouse-seo-server.js",
    "mcp:test:content": "node ./scripts/mcp-smoke.js content-audit list_pages",
    "mcp:test:seo": "node ./scripts/mcp-smoke.js lighthouse-seo audit",
    "mcp:zen": "node ./mcp-servers/zen-server.js",
    "mcp:test:zen": "node ./scripts/mcp-smoke.js zen generate_affirmation",
    "mcp:context7": "node ./scripts/run-context7.js",
    "mcp:test:context7": "node ./scripts/mcp-smoke.js context7",
    "mcp:vercel": "node ./scripts/run-vercel-mcp.js"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/validator": "^13.15.3",
    "@upstash/redis": "^1.35.4",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "resend": "^3.5.0",
    "@tanstack/react-query": "^5.71.10",
    "@tanstack/react-query-next-experimental": "^5.71.10",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "unsplash-js": "^7.0.20",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@modelcontextprotocol/sdk": "^1.18.2",
    "@playwright/test": "^1.55.1",
    "@tanstack/react-query-devtools": "^5.71.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "better-npm-audit": "^3.11.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "gray-matter": "^4.0.3",
    "kill-port": "^2.0.1",
    "lighthouse": "^12.0.0",
    "linkinator": "^6.0.5",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5",
    "wait-on": "^9.0.1"
  }
}
